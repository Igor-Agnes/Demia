import colorama
from colorama import Fore, Back, Style
colorama.init(autoreset=True)

print(Fore.BLUE+Back.YELLOW+"Hi My name is Igor Cavalcante "+ Fore.YELLOW+ Back.BLUE+"I am a Machine Learning apprentice")
print(Back.CYAN+"Hi My name is Igor Cavalcante")
print(Fore.RED + Back.GREEN+ "Hi My name is Demia")
